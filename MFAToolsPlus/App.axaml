<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="MFAToolsPlus.App"
             xmlns:local="using:MFAToolsPlus"
             xmlns:suki="https://github.com/kikipoulet/SukiUI"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->
             <Application.Resources>
                 <ResourceDictionary>
                     <ResourceDictionary.MergedDictionaries>
                         <ResourceInclude Source="avares://MFAToolsPlus/Assets/Style/Geometry.axaml" />
                     </ResourceDictionary.MergedDictionaries>
                     <ResourceDictionary.ThemeDictionaries>
                         <ResourceDictionary x:Key="Light">
                             <ResourceDictionary.MergedDictionaries>
                                 <ResourceInclude Source="avares://sukiUI/ColorTheme/Light.axaml" />
                                 <ResourceInclude Source="avares://MFAToolsPlus/Assets/Theme/LightTheme.axaml" />
                             </ResourceDictionary.MergedDictionaries>
                         </ResourceDictionary>
                         <ResourceDictionary x:Key="Dark">
                             <ResourceDictionary.MergedDictionaries>
                                 <ResourceInclude Source="avares://sukiUI/ColorTheme/Dark.axaml" />
                                 <ResourceInclude Source="avares://MFAToolsPlus/Assets/Theme/DarkTheme.axaml" />
                             </ResourceDictionary.MergedDictionaries>
                         </ResourceDictionary>
                     </ResourceDictionary.ThemeDictionaries>
                 </ResourceDictionary>

             </Application.Resources>
    <Application.Styles>
        <FluentTheme />
        <suki:SukiTheme ThemeColor="Blue" />
    </Application.Styles>
</Application>